        "zipfs-warns": "tsc -p src/zipfs/tsconfig.json",
        "zipfs-errors": "tsc -p src/tsconfig.check-zipfs.json",
        "build-zipfs": "esbuild src/zipfs/build.ts --bundle --minify --target=es6 --outfile=dist/zipfs.min.js"
    },
