{
    "compilerOptions": {
        "noEmit": true,
        "newLine": "lf",
        "target": "ES6",
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "lib": [
            "ES6",
            "ES2016.Array.Include",
            "DOM"
        ],
        "strict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true
    }
}
