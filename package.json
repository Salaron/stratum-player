{
    "name": "stratum-player",
    "version": "0.7.0",
    "description": "",
    "scripts": {
        "live": "node utils/live live",
        "unit": "node utils/live unit",
        "player": "cp dist/stratum.min.js player && http-server player",
        "ts-warns": "tsc -p src/tsconfig.json",
        "ts-errors": "tsc -p src/tsconfig.check-build.json",
        "build-types": "tsc -p src/tsconfig.build-types.json && cp dist/types/api.d.ts dist/stratum.d.ts && (echo export as namespace stratum;>> dist/stratum.d.ts)",
        "build-bundle": "esbuild src/stratum/api.ts --bundle --global-name=stratum --minify --target=es6 --outfile=dist/stratum.min.js",
        "build": "npm run build-types --silent && npm run build-bundle --silent"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/fabric": "^3.6.9",
        "@types/mocha": "^8.0.4",
        "fabric": "^4.2.0",
        "jszip": "git+https://github.com/khokm/jszip.git",
        "serve-handler": "^6.1.3",
        "stream": "0.0.2",
        "watch": "^1.0.2",
        "websocket": "^1.0.32"
    }
}
